{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "postinstall": "node scripts/fix-assets-registry.js",
    "prestart": "pnpm run kill-port",
    "start": "expo start --tunnel",
    "start:clear": "pnpm run kill-port && expo start --tunnel --clear",
    "start:tunnel": "pnpm run kill-port && pnpm exec expo start --tunnel",
    "start:fresh": "pnpm run kill-port && expo start --tunnel --clear --reset-cache",
    "start:simple": "node start-expo.js",
    "start:local": "pnpm run kill-port && expo start --clear",
    "kill-port": "node -e \"const { exec } = require('child_process'); [8081, 19000, 19001, 19002].forEach(port => exec('netstat -ano | findstr :' + port, (err, stdout) => { if (stdout) { const pid = stdout.split('\\n')[0].trim().split(/\\s+/).pop(); if (pid && pid !== '0') exec('taskkill /F /PID ' + pid, () => {}); } }));\"",
    "deploy": "pnpm run kill-port && pnpm run start:fresh",
    "deploy:simple": "node deploy.js",
    "deploy:tunnel": "node deploy.js --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "fix-deps": "node scripts/auto-fix-dependencies.js",
    "prebuild": "node scripts/pre-build-check.js",
    "prebuild:ios": "node scripts/fix-ios-deployment.js",
    "build:development": "pnpm run prebuild && eas build --profile development",
    "build:preview": "pnpm run prebuild && eas build --profile preview",
    "build:production": "pnpm run prebuild && eas build --profile production",
    "build:internal": "node build-internal.js",
    "build:share": "node build-internal.js",
    "share:app": "node share-app.js",
    "quick:deploy": "node quick-deploy.js",
    "quick:deploy:wait": "node quick-deploy.js --wait",
    "debug": "cd ../debug-tools && node supabase-debug.js",
    "debug:card": "cd ../debug-tools && npm run debug:card",
    "debug:payment": "cd ../debug-tools && npm run debug:payment",
    "debug:setup": "cd ../debug-tools && npm run setup",
    "debug:logs": "node debug-logs.js",
    "debug:session": "node debug-logs.js --session",
    "debug:errors": "node debug-logs.js --errors",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update": "eas update",

    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@expo/metro-runtime": "~3.2.3",
    "@invoicepe/types": "workspace:*",
    "@invoicepe/ui-kit": "workspace:*",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/bottom-tabs": "^7.3.15",
    "@react-navigation/native": "^7.1.11",
    "@react-navigation/stack": "^7.3.4",
    "@shopify/flash-list": "1.6.4",
    "@supabase/supabase-js": "^2.50.0",
    "babel-preset-expo": "^13.2.1",
    "dotenv": "^16.4.7",
    "expo": "~52.0.0",
    "expo-asset": "~11.0.1",
    "expo-barcode-scanner": "~14.0.0",
    "expo-build-properties": "~0.13.3",
    "expo-camera": "~16.0.0",
    "expo-constants": "~17.0.0",
    "expo-contacts": "~14.0.0",
    "expo-crypto": "~14.0.0",
    "expo-dev-client": "~5.0.0",
    "expo-haptics": "~14.0.0",
    "expo-image-picker": "~16.0.0",
    "expo-linking": "~7.0.0",
    "expo-location": "~18.0.0",
    "expo-media-library": "~17.0.0",
    "expo-notifications": "~0.29.0",
    "expo-print": "~14.0.0",
    "expo-secure-store": "~14.0.0",
    "expo-sharing": "~13.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.26.0",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-reanimated": "~3.16.0",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^4.1.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@expo/cli": "^0.21.0",
    "@expo/config-plugins": "~8.0.11",
    "@expo/metro-config": "~0.18.11",
    "@expo/prebuild-config": "~7.0.9",
    "@react-native/community-cli-plugin": "^0.76.3",
    "@types/react": "~18.3.0",
    "eas-cli": "^16.12.0",
    "metro": "~0.82.4",
    "metro-config": "~0.82.4",
    "metro-resolver": "~0.82.4",
    "typescript": "~5.3.3"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-core"
      ]
    }
  },
  "private": true
}