{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prestart": "npm run kill-port",
    "start": "expo start",
    "start:clear": "npm run kill-port && expo start --clear",
    "start:tunnel": "npm run kill-port && npx expo start --tunnel",
    "start:fresh": "npm run kill-port && expo start --clear --reset-cache",
    "start:simple": "node start-expo.js",
    "kill-port": "npx kill-port 8081 19000 19001 19002 || true",
    "deploy": "npm run kill-port && npm run start:fresh",
    "deploy:simple": "node deploy.js",
    "deploy:tunnel": "node deploy.js --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update": "eas update",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@invoicepe/types": "workspace:*",
    "@invoicepe/ui-kit": "workspace:*",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/bottom-tabs": "^7.3.15",
    "@react-navigation/native": "^7.1.11",
    "@react-navigation/stack": "^7.3.4",
    "@shopify/flash-list": "1.7.3",
    "@supabase/supabase-js": "^2.50.0",
    "dotenv": "^16.4.7",
    "expo": "~52.0.46",
    "expo-barcode-scanner": "^13.0.1",
    "expo-build-properties": "^0.13.3",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.8",
    "expo-contacts": "~14.0.5",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.20",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-location": "~18.0.10",
    "expo-media-library": "^17.0.6",
    "expo-notifications": "~0.29.14",
    "expo-print": "~14.0.3",
    "expo-secure-store": "~14.0.1",
    "expo-sharing": "~13.0.1",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^4.4.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@expo/cli": "^0.22.3",
    "@types/react": "~18.3.23",
    "eas-cli": "^16.10.1",
    "kill-port": "^2.0.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
